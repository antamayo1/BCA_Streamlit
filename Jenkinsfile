pipeline {
  agent any
  stages {
    stage('Hello!') {
      steps {
        echo 'My name is Aaron and I am learning Jenkins!'
      }
    }
    stage('Hi!') {
      steps {
        echo 'Hello Aaron, welcome to Jenkins!'
      }
    }
    stage('Bye!') {
      steps {
        echo 'Goodbye Aaron, see you next time!'
      }
    }
  }
}